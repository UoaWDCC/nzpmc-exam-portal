{
    "name": "nzpmc-backend",
    "version": "0.0.3",
    "description": "NZPMC Backend GraphQL API",
    "scripts": {
        "start": "npm run starter",
        "starter": "ts-node src/index.ts",
        "dev": "nodemon -e .js,.ts,.json --watch src .env --exec \"tsc --incremental && node bin/src/index.js\"",
        "jwt": "ts-node ./test/scripts/loginScript mdLy2GYwTMZovNtnkj121dWU2YP2",
        "lint": "eslint . --ext .js,.ts",
        "fix": "eslint . --fix --ext .js,.ts",
        "build": "tsc",
        "docker": "docker build . -t nzpmc-backend:latest",
        "test": "jest",
        "prestart": "npm run generate",
        "predev": "npm run generate",
        "generate": "graphql-codegen",
        "emulator": "firebase emulators:start"
    },
